          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".underline": {
              "padding-top": ".5rem",
              ".line": {
                "width": "5rem",
                "height": "1px",
                "display": "inline-block",
                "background-color": "@underlineColor",
                "transition": "width .3s"
              }
            },
            ".title-wrap:hover": {
              ".line": {
                "width": "6rem"
              }
            },
            "& when (@showTitle), (@showSubtitle)": {
              ".title-wrap": {
                "padding-bottom": "2rem",
                "margin": "auto"
              }
            },
            "& when (@showTitle) and (@showSubtitle)": {
              ".mbr-section-subtitle": {
                "padding-top": "1rem"
              }
            },
            "amp-img": {
              "transition": "all .5s",
              "z-index": "1",
              "&:hover": {
                "opacity": ".3",
                "cursor": "pointer"
              }
            },
            ".image-wrap": {
              "padding": "0",
              "overflow": "hidden",
              "position": "relative"
            },
            "& when (@showCaption)": {
              ".lines": {
                "position": "absolute",
                "top": "1.5rem",
                "bottom": "1.5rem",
                "left": "1.5rem",
                "right": "1.5rem",
                "opacity": "0",
                "z-index": "0",
                "transition": "opacity 1s"
              },
              ".text-wrap": {
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "opacity": "0",
                "-webkit-transform": "translate(-50%, -50%)",
                "transform": "translate(-50%, -50%)",
                "z-index": "2",
                "transition": "opacity .5s",
                "text-align": "center"
              },
              ".lines:after, .lines:before": {
                "position": "absolute",
                "top": "0",
                "bottom": "0",
                "right": "0",
                "left": "0",
                "content": "''",
                "opacity": "0",
                "transition": "all .5s",
                "z-index": "1"
              },
              ".lines:before": {
                "border-top": "1px solid @linesColor",
                "border-bottom": "1px solid @linesColor",
                "-webkit-transform": "scale(0, 1)",
                "transform": "scale(0, 1)"
              },
              ".lines:after": {
                "border-left": "1px solid @linesColor",
                "border-right": "1px solid @linesColor",
                "-webkit-transform": "scale(1, 0)",
                "transform": "scale(1, 0)"
              },
              ".image-wrap:hover": {
                ".lines:after, .lines:before": {
                  "opacity": "1",
                  "-webkit-transform": "scale(1)",
                  "transform": "scale(1)"
                },
                ".lines, .text-wrap": {
                  "opacity": "1"
                },
                "amp-img": {
                  "opacity": ".3",
                  "cursor": "pointer"
                }
              },
              ".card-text": {
                "word-break": "break-word"
              }
            },
            "@media (min-width: 768px)": {
              "amp-img": {
                "height": "100%",
                "img": {
                  "height": "100%",
                  "object-fit": "cover",
                  "object-position": "center center"
                }
              }
            },
            "a.control": {
              "position": "absolute",
              "width": "16px",
              "height": "16px",
              "right": "48px",
              "top": "32px",
              ".close": {
                "position": "relative",
                "width": "32px",
                "height": "32px"
              },
              ".close:before, .close:after": {
                "position": "absolute",
                "left": "15px",
                "content": "' '",
                "height": "33px",
                "width": "2px",
                "background-color": "#fff"
              },
              ".close:before": {
                "-webkit-transform": "rotate(45deg)",
                "transform": "rotate(45deg)"
              },
              ".close:after": {
                "-webkit-transform": "rotate(-45deg)",
                "transform": "rotate(-45deg)"
              }
            },
            "SPAN": {
              "color": "#000000",
              "text-align": "left"
            }
          },
